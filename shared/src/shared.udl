namespace shared {
  [Throws=PlatformError]
  string add_for_platform(u32 left, u32 right, Platform platform);
};

dictionary CatFactData {
  string fact;
  i32 length;
};

interface CatFact {
  constructor(CatFactData fact);
  string format();
};

[Error]
enum PlatformError {
  "InternalPlatformError",
};

callback interface Platform {
  [Throws=PlatformError]
  string get();
};
